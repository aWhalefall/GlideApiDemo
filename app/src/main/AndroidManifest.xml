<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dragger2.nuoyuan.com.glideapidemo">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.SimpleActivity"/>
        <activity android:name=".activity.NormalActivity"/>
        <activity android:name=".activity.SimpleGalleryActivity"/>
        <activity android:name=".activity.HardActivity"/>
        <activity android:name=".activity.GalleryUseRecyclerViewActivity"/>

        <meta-data
            android:name="dragger2.nuoyuan.com.glideapidemo.glideApiTest.GlideModuleTest"
            android:value="GlideModule" />
    </application>

</manifest>